{
  "name": "recast.ly",
  "version": "1.1.0",
  "description": "Making YouTube with React and ES6",
  "author": "Hack Reactor <curriculum.team@hackreactor.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "build": "webpack",
    "test": "jest --watch --runInBand --env jest-environment-node-debug"
  },
  "dependencies": {
    "access-log": "^0.4.1",
    "bootstrap": "4.0.0-alpha.6",
    "finalhandler": "^1.1.0",
    "geoip-lite": "^1.2.1",
    "http": "^0.0.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "serve-static": "^1.13.1",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "jest": "^21.2.1",
    "jest-environment-node-debug": "^2.0.0",
    "jsdom": "^11.2.0",
    "live-server": "^1.2.0",
    "raf": "^3.3.2",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.0.0",
    "superagent-mock": "^3.6.0",
    "webpack": "^3.5.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/spec"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/spec/config/jest.config.js",
    "testMatch": [
      "**/?(*)(Spec).js?(x)"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/jsdom/.*",
      "<rootDir>/node_modules/sinon/.*",
      "<rootDir>/node_modules/react/.*",
      "<rootDir>/.*/components/.*"
    ]
  }
}
